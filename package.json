    "@typescript-eslint/parser": "^5.6.0",
    "@vitejs/plugin-vue": "^1.10.1",
    "autoprefixer": "^10.4.0",
    "axios": "^0.24.0",
    "browserslist": "^4.17.6",
    "cross-env": "^7.0.3",
    "dexie": "^3.2.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-vue": "^8.2.0",
    "feather-icons": "^4.28.0",
    "husky": "^7.0.0",
    "mitt": "^3.0.0",
